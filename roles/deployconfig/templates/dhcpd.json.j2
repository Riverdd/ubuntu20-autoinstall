option domain-name "autopxe.com";
option domain-name-servers srv.autopxe.com;
default-lease-time 600;
max-lease-time 1200;
ddns-update-style none;
subnet 192.168.100.0 netmask 255.255.255.0 {
    range 192.168.100.50 192.168.100.250;
    option routers 192.168.100.20;
    option subnet-mask 255.255.255.0;
    option domain-name-servers 192.168.100.20;
    option ntp-servers 192.168.100.20;
    option netbios-name-servers 192.168.100.20;
    option broadcast-address 192.168.100.255;
    next-server 192.168.100.20;
    filename "pxelinux.0";
    allow booting;
    allow bootp;
}
